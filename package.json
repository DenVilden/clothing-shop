{
  "name": "clothing-shop",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "type": "module",
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "axios": "0.18.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "firebase": "^6.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "stripe": "^7.7.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0"
  },
  "scripts": {
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "nodemon --experimental-modules server.js",
    "serve": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node --experimental-modules server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "react/jsx-sort-props": 2,
      "jsx-a11y/label-has-for": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
